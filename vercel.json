{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd somema-ai && npm run build",
  "outputDirectory": "somema-ai/.next",
  "installCommand": "cd somema-ai && npm install",
  "crons": [
    {
      "path": "/api/cron/post-scheduler",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/ads-api-trigger",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "somema-ai/app/api/cron/post-scheduler/route.ts": {
      "maxDuration": 30
    },
    "somema-ai/app/api/cron/ads-api-trigger/route.ts": {
      "maxDuration": 30
    }
  }
} 